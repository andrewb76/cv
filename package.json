    "@cypress/webpack-dev-server": "^2.4.1",
    "@sentry/tracing": "^7.17.4",
    "@sentry/vue": "^7.17.4",
    "@types/ramda": "^0.28.18",
    "autoprefixer": "^10",
    "core-js": "^3.8.3",
    "cypress": "^10.10.0",
    "d3": "^7.6.1",
    "daisyui": "^2.38.0",
    "pinia": "^2.0.23",
    "postcss": "^8",
    "ramda": "^0.28.0",
    "tailwindcss": "^3",
    "vue": "^3.2.41",
    "vue-i18n": "^9.1.0",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "@types/html-webpack-plugin": "^3.2.6",
    "html-webpack-plugin": "^5.5.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "json": {
